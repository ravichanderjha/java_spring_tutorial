<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="address1" class="com.javatpoint.Address">
<property name="addressLine1" value="Baljeet Nagar"></property>
<property name="city" value="New Delhi"></property>
<property name="state" value="Delhi"></property>
<property name="country" value="India"></property>
</bean>

<bean id="obj" class="com.javatpoint.Employee">
<property name="id" value="1"></property>
<property name="name" value="Ravi Chander Jha"></property>
<property name="address" ref="address1"></property>
</bean>

</beans>